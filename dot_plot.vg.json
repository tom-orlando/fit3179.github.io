{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "background": "white",
  "padding": 5,
  "width": 800,
  "height": 600,
  "title": {"text": "Property sales by NYC boroughs", "frame": "group"},
  "style": "cell",
  "data": [
    {"name": "borough_highlight_store"},
    {
      "name": "source_0",
      "url": "https://raw.githubusercontent.com/tom-orlando/tom-orlando.github.io/master/assignment_2/week_10/nyc-rolling-sales-clean_v4.csv",
      "format": {"type": "csv", "delimiter": ","},
      "transform": [
        {"type": "filter", "expr": "datum.gross_sq_ft > 0"},
        {"type": "filter", "expr": "datum.total_unit > 0"},
        {"type": "filter", "expr": "datum.price > Price_Above"},
        {
          "type": "filter",
          "expr": "Borough_selection == null || datum.borough == Borough_selection"
        },
        {
          "type": "formula",
          "expr": "datum.price/datum.gross_sq_ft",
          "as": "Price per gross square feet"
        }
      ]
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"gross_sq_ft\"]) && isFinite(+datum[\"gross_sq_ft\"]) && isValid(datum[\"price\"]) && isFinite(+datum[\"price\"]) && isValid(datum[\"total_unit\"]) && isFinite(+datum[\"total_unit\"])"
        }
      ]
    },
    {
      "name": "data_1",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"gross_sq_ft\"]) && isFinite(+datum[\"gross_sq_ft\"]) && isValid(datum[\"price\"]) && isFinite(+datum[\"price\"])"
        }
      ]
    }
  ],
  "signals": [
    {
      "name": "unit",
      "value": {},
      "on": [
        {"events": "mousemove", "update": "isTuple(group()) ? group() : unit"}
      ]
    },
    {
      "name": "borough_highlight_borough_legend",
      "value": null,
      "on": [
        {
          "events": [
            {
              "source": "view",
              "type": "click",
              "markname": "borough_legend_symbols"
            },
            {
              "source": "view",
              "type": "click",
              "markname": "borough_legend_labels"
            },
            {
              "source": "view",
              "type": "click",
              "markname": "borough_legend_entries"
            }
          ],
          "update": "datum.value || item().items[0].items[0].datum.value",
          "force": true
        },
        {
          "events": [{"source": "view", "type": "click"}],
          "update": "!event.item || !datum ? null : borough_highlight_borough_legend",
          "force": true
        }
      ]
    },
    {
      "name": "borough_highlight",
      "update": "vlSelectionResolve(\"borough_highlight_store\", \"union\", true)"
    },
    {
      "name": "Price_Above",
      "value": 0,
      "bind": {
        "input": "range",
        "min": 0,
        "max": 100000000,
        "step": 1000,
        "name": "Minimum Price: "
      }
    },
    {
      "name": "Borough_selection",
      "bind": {
        "input": "select",
        "options": [null, "1", "2", "3", "4", "5"],
        "labels": ["Show All", "1", "2", "3", "4", "5"],
        "name": "Borough Selection: "
      }
    },
    {
      "name": "borough_highlight_tuple",
      "update": "borough_highlight_borough_legend !== null ? {fields: borough_highlight_tuple_fields, values: [borough_highlight_borough_legend]} : null"
    },
    {
      "name": "borough_highlight_tuple_fields",
      "value": [{"type": "E", "field": "borough"}]
    },
    {
      "name": "borough_highlight_toggle",
      "value": false,
      "on": [
        {
          "events": {"merge": [{"source": "view", "type": "click"}]},
          "update": "event.shiftKey"
        }
      ]
    },
    {
      "name": "borough_highlight_modify",
      "on": [
        {
          "events": {"signal": "borough_highlight_tuple"},
          "update": "modify(\"borough_highlight_store\", borough_highlight_toggle ? null : borough_highlight_tuple, borough_highlight_toggle ? null : true, borough_highlight_toggle ? borough_highlight_tuple : null)"
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "layer_0_marks",
      "type": "symbol",
      "style": ["circle"],
      "interactive": true,
      "from": {"data": "data_0"},
      "encode": {
        "update": {
          "opacity": [
            {
              "test": "!(length(data(\"borough_highlight_store\"))) || (vlSelectionTest(\"borough_highlight_store\", datum))",
              "value": 0.6
            },
            {"value": 0.2}
          ],
          "fill": {"scale": "color", "field": "borough"},
          "tooltip": {
            "signal": "{\"neighborhood\": isValid(datum[\"neighborhood\"]) ? datum[\"neighborhood\"] : \"\"+datum[\"neighborhood\"], \"gross_sq_ft\": format(datum[\"gross_sq_ft\"], \",\"), \"price\": format(datum[\"price\"], \",\"), \"total_unit\": format(datum[\"total_unit\"], \",\"), \"Price per gross sq ft\": format(datum[\"Price per gross sq ft\"], \".2f\")}"
          },
          "ariaRoleDescription": {"value": "circle"},
          "description": {
            "signal": "\"Gross Square Feet: \" + (format(datum[\"gross_sq_ft\"], \"\")) + \"; price: \" + (format(datum[\"price\"], \"\")) + \"; total_unit: \" + (format(datum[\"total_unit\"], \".1s\")) + \"; borough: \" + (isValid(datum[\"borough\"]) ? datum[\"borough\"] : \"\"+datum[\"borough\"]) + \"; neighborhood: \" + (isValid(datum[\"neighborhood\"]) ? datum[\"neighborhood\"] : \"\"+datum[\"neighborhood\"]) + \"; gross_sq_ft: \" + (format(datum[\"gross_sq_ft\"], \",\")) + \"; Price per gross sq ft: \" + (format(datum[\"Price per gross sq ft\"], \".2f\"))"
          },
          "x": {"scale": "x", "field": "gross_sq_ft"},
          "y": {"scale": "y", "field": "price"},
          "size": {"scale": "size", "field": "total_unit"},
          "shape": {"value": "circle"}
        }
      }
    },
    {
      "name": "layer_1_marks",
      "type": "text",
      "style": ["text"],
      "interactive": true,
      "from": {"data": "data_1"},
      "encode": {
        "update": {
          "align": {"value": "right"},
          "baseline": {"value": "middle"},
          "dx": {"value": -12},
          "fontSize": {"value": 11.5},
          "fontStyle": {"value": "italic"},
          "fill": {"value": "black"},
          "opacity": [
            {
              "test": "datum['neighbourhood'] == 'Harlem-Central' || datum['neighbourhood'] == 'Kips Bay'",
              "value": 1
            },
            {"value": 0}
          ],
          "tooltip": {
            "signal": "{\"neighborhood\": isValid(datum[\"neighborhood\"]) ? datum[\"neighborhood\"] : \"\"+datum[\"neighborhood\"], \"gross_sq_ft\": format(datum[\"gross_sq_ft\"], \",\"), \"price\": format(datum[\"price\"], \",\"), \"total_unit\": format(datum[\"total_unit\"], \",\"), \"Cases per 10,000 Population\": format(datum[\"Cases per 10,000 Population\"], \".2f\")}"
          },
          "description": {
            "signal": "\"Gross Square Feet: \" + (format(datum[\"gross_sq_ft\"], \"\")) + \"; price: \" + (format(datum[\"price\"], \"\")) + \"; neighborhood: \" + (isValid(datum[\"neighborhood\"]) ? datum[\"neighborhood\"] : \"\"+datum[\"neighborhood\"]) + \"; gross_sq_ft: \" + (format(datum[\"gross_sq_ft\"], \",\")) + \"; total_unit: \" + (format(datum[\"total_unit\"], \",\")) + \"; Cases per 10,000 Population: \" + (format(datum[\"Cases per 10,000 Population\"], \".2f\"))"
          },
          "x": {"scale": "x", "field": "gross_sq_ft"},
          "y": {"scale": "y", "field": "price"},
          "text": {
            "signal": "isValid(datum[\"neighborhood\"]) ? datum[\"neighborhood\"] : \"\"+datum[\"neighborhood\"]"
          }
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "log",
      "domain": [100, 1200000],
      "range": [0, {"signal": "width"}],
      "nice": true
    },
    {
      "name": "y",
      "type": "log",
      "domain": [1, 2500000000],
      "range": [{"signal": "height"}, 0],
      "nice": true
    },
    {
      "name": "color",
      "type": "ordinal",
      "domain": ["1", "2", "3", "4", "5"],
      "range": ["#e41a1c", "#984ea3", "#ff7f00", "#a6cee3", "#377eb8"]
    },
    {
      "name": "size",
      "type": "threshold",
      "domain": [10, 50, 100, 500, 1000],
      "range": {
        "signal": "sequence(9, 361 + (361 - 9) / (6 - 1), (361 - 9) / (6 - 1))"
      }
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "tickCount": 7,
      "gridScale": "y",
      "grid": true,
      "domain": false,
      "labels": false,
      "aria": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "tickCount": 6,
      "gridScale": "x",
      "grid": true,
      "domain": false,
      "labels": false,
      "aria": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "Gross Square Feet",
      "tickCount": 7,
      "labelFlush": true,
      "labelOverlap": "greedy",
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Price",
      "tickCount": 6,
      "labelOverlap": "greedy",
      "zindex": 0
    }
  ],
  "legends": [
    {
      "fill": "color",
      "symbolType": "circle",
      "title": "borough",
      "encode": {
        "labels": {
          "name": "borough_legend_labels",
          "interactive": true,
          "update": {
            "opacity": [
              {
                "test": "(!length(data(\"borough_highlight_store\")) || (borough_highlight[\"borough\"] && indexof(borough_highlight[\"borough\"], datum.value) >= 0))",
                "value": 1
              },
              {"value": 0.35}
            ]
          }
        },
        "symbols": {
          "name": "borough_legend_symbols",
          "interactive": true,
          "update": {
            "opacity": [
              {
                "test": "(!length(data(\"borough_highlight_store\")) || (borough_highlight[\"borough\"] && indexof(borough_highlight[\"borough\"], datum.value) >= 0))",
                "value": 0.6
              },
              {"value": 0.35}
            ]
          }
        },
        "entries": {
          "name": "borough_legend_entries",
          "interactive": true,
          "update": {"fill": {"value": "transparent"}}
        }
      }
    },
    {
      "format": ".1s",
      "size": "size",
      "labelOverlap": "greedy",
      "symbolType": "circle",
      "title": "total_unit",
      "encode": {
        "symbols": {
          "update": {
            "fill": {"value": "black"},
            "fillOpacity": {"value": 0.6},
            "opacity": {"value": 0.6},
            "stroke": {"value": "transparent"}
          }
        }
      }
    }
  ],
  "config": {}
}